{
  "name": "mapbox-embed-vue",
  "version": "0.1.0",
  "scripts": {
    "dev": "pnpm run --parallel dev",
    "build": "pnpm run --parallel build",
    "preview": "pnpm run --parallel preview",
    "test": "pnpm run --parallel test",
    "lint": "pnpm run --parallel lint",
    "lint:fix": "pnpm run --parallel lint:fix",
    "typecheck": "pnpm run --parallel typecheck",
    "prepublish": "pnpm --filter @mxwllstn/mapbox-embed-vue run prepublish",
    "release": "pnpm run lint && pnpm run test && pnpm run build && pnpm --filter @mapbox-embed-vue/cli run release && pnpm --filter @mxwllstn/mapbox-embed-vue run release",
    "compile": "pnpm --filter @mapbox-embed-vue/cli run compile"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.15.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.29.0",
    "typescript": "^5.8.3",
    "vue": "^3.5.17"
  }
}
